{
  "name": "vsts-extension-angular-seed",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "package": "tfx extension create  --rev-version",
    "gallery-publish": "tfx extension publish --rev-version",
    "clean": "rimraf ./dist && rimraf ./*.vsix",
    "package-debug": "tfx extension create --rev-version --override {\\\"baseUrl\\\": \\\"https://localhost:44300\\\"}",
    "publish-package": "npm run clean && npm run build-prod && npm run package && npm run gallery-publish",
    "publish-debug-package": "npm run clean && npm run build && npm run package-debug && npm run gallery-publish",
    "debug": "concurrently \"ws --https -p 44300\" \"npm run watch\"",
    "build": "webpack",
    "build-prod": "webpack --env=\"prod\"",
    "watch": "webpack --watch",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./protractor.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@types/jquery": "^2.0.34",
    "@types/q": "0.0.32",
    "applicationinsights-js": "^1.0.17",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.2",
    "tippy.js": "^2.5.2",
    "vss-web-extension-sdk": "^5.134.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.4.5",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "autoprefixer": "^6.5.3",
    "circular-dependency-plugin": "^3.0.0",
    "codelyzer": "~3.2.0",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.0.1",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.10.0",
